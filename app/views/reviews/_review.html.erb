<div id="<%= dom_id review %>">
  <p>
    <strong>Id:</strong>
    <%= review.id %>
  </p>

  <p>
    <strong>Restaurant:</strong>
    <%= review.restaurant_id %>
  </p>

  <p>
    <strong>Halal item:</strong>
    <%= review.halal_item_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= review.user_id %>
